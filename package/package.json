{
    "name": "google-books-search",
    "version": "1.0.0",
    "description": "CS Bootcamp Homework Google Books Search with React",
    "main": "server.js",
    "scripts": {
      "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
      "start:prod": "node server.js",
      "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\" ",
      "client": "cd client && npm run start",
      "install": "cd client && npm install",
      "build": "cd client && npm run build",
      "heroku-postbuild": "npm run build"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/vegerpena20/GoogleBooks.git"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
      "concurrently": "^4.1.0",
      "nodemon": "^1.18.7"
    },
    "bugs": {
      "url": "https://github.com/vegerpena20/GoogleBooks.git/issues"
    },
    "dependencies": {
      "aws-sdk": "^2.585.0",
      "axios": "^0.18.1",
      "child_process": "^1.0.2",
      "dotenv": "^8.2.0",
      "eslint": "^6.1.0",
      "express": "^4.16.3",
      "fs": "0.0.1-security",
      "if-env": "^1.0.4",
      "mongoose": "^5.3.16",
      "tls": "0.0.1"
    },
    "homepage": "https://github.com/vegerpena20/GoogleBooks.git#readme"
  }